{
  "name": "skedulo-packaging-sdk",
  "version": "1.0.0",
  "description": "",
  "main": "./app/index.js",
  "homepage": "https://new.skedulo.com",
  "author": "Skedulo <support@skedulo.com>",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "bootstrap": "cd src && yarn install --pure-lockfile && cd ../ && yarn install --pure-lockfile --ignore-engines",
    "node-run": "node --turbo --ignition",
    "node-run-dev": "yarn node-run -r ts-node/register",
    "dev-node": "nodemon --watch src -e ts,js -x 'yarn node-run-dev' src/server/server.ts",
    "copy-ngrok": "node scripts/copyNgrok.js",
    "copy-static": "node scripts/copyAssets.js && yarn run copy-ngrok",
    "compile-scripts": "tsc",
    "compile-styles": "node-sass src/scss/main.scss app/scss/main.css",
    "watch-scripts": "tsc --watch",
    "watch-styles": "yarn compile-styles --watch",
    "compile": "yarn compile-scripts && yarn compile-styles && yarn copy-static",
    "start-ui": "electron ./",
    "lint": "tslint --project ./",
    "start": "rm -rf app && rm -rf dist && yarn compile && electron ./",
    "pack": "yarn compile && cd app && electron-builder --dir",
    "dist": "yarn compile && cd app && electron-builder",
    "dev": "yarn copy-static && yarn compile-styles && node ./tools/watch-web.js",
    "dev-ui": "cross-env NODE_ENV=development nodemon -x yarn start-ui ./",
    "postinstall": "ts-node ./download-ngrok-bins.ts && yarn compile"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/tests/**/*.test.(ts|tsx)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": [
      "<rootDir>/src/tests/jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/src/node_modules/enzyme-to-json/serializer"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "coverageDirectory": "<rootDir>/src/tests/coverage",
    "collectCoverageFrom": [
      "src/web/**/*.{ts,tsx}"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.json"
      }
    }
  },
  "nodemonConfig": {
    "watch": [
      "app/"
    ],
    "ignore": [
      "app/web/"
    ],
    "ext": "js"
  },
  "build": {
    "appId": "com.skedulo.sdk",
    "artifactName": "skedulo-sdk.${ext}",
    "extraResources": [
      "app/assets/ngrok-*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "zip"
    },
    "win": {
      "target": "portable"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/cross-spawn": "^6.0.0",
    "@types/enzyme": "^3.10.3",
    "@types/fs-extra": "^11.0.1",
    "@types/get-port": "^4.0.1",
    "@types/highlight.js": "^9.12.1",
    "@types/http-proxy": "^1.12.2",
    "@types/joi": "^13.0.0",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/koa": "2.13.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json-error": "^3.1.1",
    "@types/koa-logger": "^2.0.2",
    "@types/koa-router": "^7.0.23",
    "@types/koa-send": "^3.3.0",
    "@types/lodash": "^4.14.138",
    "@types/logfmt": "^1.2.1",
    "@types/marked": "^0.3.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-test-renderer": "^16.9.0",
    "@types/request-promise": "^4.1.39",
    "@types/semver": "^5.5.0",
    "@types/tar": "^4.0.3",
    "@types/jest": "^22.1.3",
    "@types/koa__cors": "^3.1.1",
    "@types/node": "12.20.3",
    "@types/vary": "^1.1.0",
    "cross-env": "^5.2.0",
    "decompress-zip": "^0.2.2",
    "devtron": "1.4.0",
    "electron": "^14.2.9",
    "electron-builder": "^23.6.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.4.0",
    "jest": "^24.9.0",
    "nodemon": "^1.15.1",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^10.9.1",
    "tslint": "^5.19.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@skedulo/packaging-internal-commons": "file:./packages/@skedulo/packaging-internal-commons",
    "@skedulo/sked-commons": "file:./packages/@skedulo/sked-commons",
    "@electron/remote": "^2.0.9",
    "@koa/cors": "^3.3.0",
    "@skedulo/sked-ui": "^19.4.1",
    "bourbon": "^4.3.4",
    "classnames": "^2.2.5",
    "cross-spawn": "^7.0.0",
    "electron-log": "^4.4.6",
    "fix-path": "^2.1.0",
    "fs-extra": "^11.1.0",
    "get-port": "^5.0.0",
    "highlight.js": "^9.12.0",
    "http-proxy": "^1.16.2",
    "ip": "^1.1.5",
    "joi": "12",
    "jsonwebtoken": "^8.1.0",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.1",
    "koa-json-error": "^3.1.2",
    "koa-logger": "^3.1.0",
    "koa-router": "^7.2.1",
    "koa-send": "^4.1.1",
    "lodash": "^4.17.15",
    "logfmt": "^1.3.2",
    "marked": "^0.3.6",
    "ms": "^2.0.0",
    "node-sass": "^8.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-split": "^2.0.4",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "rxjs": "5",
    "semver": "^5.5.0",
    "shelljs": "^0.8.5",
    "tar": "^4.0.2"
  }
}
